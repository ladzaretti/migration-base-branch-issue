{
  "extends": [
    "config:base"],
  "lockFileMaintenance": {
    "enabled": false
  },
  "onboarding": false,
	"pinVersions": false,
  "semanticCommits": "enabled",
  "timezone": "US/Central",
  "packageRules": [
    {
      "matchPackageNames": [
        "helm"
      ],
      "extractVersion": "^v(?<version>.*)$",
      "schedule": [
        "at any time"
      ]
    }
  ]
}
